<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent" android:background="@drawable/menunobuttons"
	android:weightSum="1">

	<Button android:id="@+id/IVthreebythree" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:background="@drawable/threebythree"
		android:layout_marginLeft="35dp" android:layout_marginTop="150dp"/>
	
	
	<Button android:background="@drawable/fourbyfour"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:id="@+id/IVfourbyfour" android:layout_marginLeft="35dp"
		android:layout_marginTop="10dp"></Button>
	<Button android:background="@drawable/bmenu"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:id="@+id/IVbmenu" android:layout_marginLeft="35dp" android:layout_marginTop="10dp"></Button>

</LinearLayout>
